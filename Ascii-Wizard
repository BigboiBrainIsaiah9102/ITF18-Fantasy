def Wizard():
  print("        /\       " )
  print("       /  \      " )
  print("     ( O O  )    " )
  print("     _| ~  |_       ")
  print("    // |  | \\\       ")
  print("   //  |  | =\\\==---- " )
  print("       |  |           " )
  print("      // \\\       " )
  print("     //   \\\      " )
  print("EVIL WIZARD")
  print("")
  print("")
Wizard()  
